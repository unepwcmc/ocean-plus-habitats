<%= render '/global/header-home' %>

<%= render partial: '/global/nav-tertiary',
  locals: {
    nav_tertiary: nav_tertiary
  }
%>

<%= render partial: '/site/partials/map',
  locals: {
    id: "section-#{t('home.nav_sticky')[0][:id]}",
    datasets: @map_datasets,
    modal_content: habitats_present_modal
  }
%>

<%= render partial: '/site/partials/proportion-protected',
  locals: {
    id: "section-#{t('home.nav_sticky')[1][:id]}",
    map_datasets: @map_datasets_habitats,
    eez_map_datasets: @eez_map_datasets,
    modal_content: habitats_protected_modal,
    eez_modal_content: eez_map_modal
  }
%>

<%= render partial: '/site/partials/habitat-cover',
  locals: {
    id: "section-#{t('home.nav_sticky')[2][:id]}",
    habitat_cover: @habitat_cover, modal_content: habitat_cover_modal
  }
%>

<%= render partial: '/site/partials/red-list',
  locals: {
    id: "section-#{t('home.nav_sticky')[3][:id]}",
    modal_content: red_list_modal
  }
%>

<%= render partial: '/site/partials/sdg',
  locals: {
    id: "section-#{t('home.nav_sticky')[4][:id]}",
    doughnut_chart: @doughnut_chart.to_json
  }
%>

<%= render partial: '/global/downloads',
  locals: {
    downloads: t('home.ancillary.downloads'),
  }
%>

<%= render '/site/partials/citation' %>